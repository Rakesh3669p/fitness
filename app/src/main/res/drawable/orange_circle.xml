<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:width="60dp"
        android:height="60dp"
        android:gravity="center">
        <shape android:shape="oval"
            >
            <solid android:color="@color/white"/>

        </shape>
    </item>
    <item
        android:gravity="center"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"
        android:bottom="5dp">

        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@color/themeOrange"/>
        </shape>

    </item>

</layer-list>